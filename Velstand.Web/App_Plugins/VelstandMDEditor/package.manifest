{
    propertyEditors: [
        {
			alias: "Velstand.MDEditor",
			name: "Velstand MarkDown Editor",
			editor: {
				view: "~/App_Plugins/VelstandMDEditor/view.html",
				hideLabel: true,
				valueType: "TEXT",
			},
			prevalues: {
				fields: [
					{
						label: "Height",
						description: "Editor Height",
						key: "editor_height",
						view: "number",
					},
				]
			},
			defaultConfig: {
				editor_height: "500",
			},
        }
    ],
    javascript: [
		'~/App_Plugins/VelstandMDEditor/assets/jquery.selection.js',
		'~/App_Plugins/VelstandMDEditor/assets/marked.js',
		'~/App_Plugins/VelstandMDEditor/assets/reMarked.js',
		'~/App_Plugins/VelstandMDEditor/assets/markedOptions.js',
		'~/App_Plugins/VelstandMDEditor/assets/Velstand_TextArea.js',
		'~/App_Plugins/VelstandMDEditor/assets/Velstand_Markdown.js',
        '~/App_Plugins/VelstandMDEditor/controller.js',
        '~/App_Plugins/VelstandMDEditor/dialogs/controller.js',
    ],
	css: [
		'~/App_Plugins/VelstandMDEditor/assets/icomoon/style.css',
		'~/App_Plugins/VelstandMDEditor/style.css',
		'~/css/style_mobile.css',
	]
}